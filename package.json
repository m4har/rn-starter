{
  "name": "starter",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "android": "react-native run-android",
    "android:release": "yarn android --variant=release",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "typecheck": "tsc-silent -p src/tsconfig.json --suppressConfig src/tsc-silent.config.js",
    "prettier": "prettier --check \"src/**/*.{ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{ts,tsx}\""
  },
  "dependencies": {
    "i18next": "^19.1.0",
    "moment": "^2.24.0",
    "react": "16.11.0",
    "react-i18next": "^11.2.7",
    "react-native": "0.62.0-rc.1",
    "react-native-appearance": "^0.3.2",
    "react-native-localize": "^1.3.2",
    "styled-components": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^0.0.5",
    "@types/react": "^16.9.17",
    "@types/react-native": "^0.60.30",
    "@types/styled-components": "^4.4.2",
    "babel-jest": "^24.9.0",
    "eslint": "^6.8.0",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "prettier": "^1.19.1",
    "react-native-typescript-transformer": "^1.2.13",
    "react-test-renderer": "^16.12.0",
    "tsc-silent": "^1.2.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
